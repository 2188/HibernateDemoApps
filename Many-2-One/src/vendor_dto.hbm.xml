<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.many2one.Vendor_DTO" table="Vendor">

		<meta attribute="class-description">This class contains VENDORS DETAILS</meta>

		<id name="vendorID" column="vendorID">
			<generator class="assigned" />
		</id>

		<property name="vendorName" type="string" column="V_NAME" />

	</class>

</hibernate-mapping>
